html
  height 100%

body
  height 100%


.content
  height 100%

  .map
    height 100%


ul.tumbler
  position fixed
  left 0
  top 0

  li
    overflow hidden
    margin-bottom 10px
    border 1px solid
    a
      display block
      padding 10px
      background-color rgba(255, 255, 255, 0.4)
      text-decoration none
      color #000000

      &:active, &.active
        background-color rgba(255, 255, 255, 0.8)
